{
  "note": "Prefer editing project.json, which the app loads automatically.",
  "name": "sample_project",
  "sample_rate": 192000,
  "ways": [
    {
      "name": "TT",
      "file": "input/TT.frd",
  "color": "green",
      "filters": [
        { "type": "linkwitz-riley", "mode": "lowpass", "order": 4, "freq": 320 }
      ]
    },
    {
      "name": "MT",
      "file": "input/MT.frd",
  "color": "blue",
      "filters": [
        { "type": "butterworth", "mode": "bandpass", "order": 4, "freqs": [280, 2500] }
      ]
    },
    {
      "name": "HT",
      "file": "input/HT.frd",
  "color": "yellow",
      "filters": [
        { "type": "peq", "f0": 6000, "gain_db": -3, "q": 2.0 },
        { "type": "phase", "f0": 4500, "q": 0.7 },
        { "type": "gain", "gain_db": 2.5 },
        { "type": "delay", "delay_us": 75 }
      ]
    }
  ]
}
